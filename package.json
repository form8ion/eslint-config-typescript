{
  "name": "@form8ion/eslint-config-typescript",
  "description": "shareable ESLint config for TypeScript projects",
  "license": "MIT",
  "version": "0.0.0-semantically-released",
  "files": [
    "example.js",
    "index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "runkitExampleFilename": "./example.js",
  "repository": "form8ion/eslint-config-typescript",
  "bugs": "https://github.com/form8ion/eslint-config-typescript/issues",
  "homepage": "https://npm.im/@form8ion/eslint-config-typescript",
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org)",
  "scripts": {
    "test": "npm-run-all --print-label --parallel lint:*",
    "lint:lockfile": "lockfile-lint",
    "lint:md": "remark . --frail",
    "lint:js": "eslint . --cache",
    "generate:md": "remark . --output",
    "lint:sensitive": "ban",
    "prepare": "husky install",
    "lint:peer": "npm ls >/dev/null"
  },
  "devDependencies": {
    "@form8ion/commitlint-config": "1.0.55",
    "@form8ion/eslint-config": "6.0.3",
    "@form8ion/remark-lint-preset": "6.0.0",
    "ban-sensitive-files": "1.10.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "8.0.3",
    "lockfile-lint": "4.12.0",
    "npm-run-all2": "6.0.6",
    "remark-cli": "11.0.0",
    "remark-toc": "8.0.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": "^4.4.2 || ^5.0.0"
  }
}
